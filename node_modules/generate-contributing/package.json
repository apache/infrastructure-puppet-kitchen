{
  "name": "generate-contributing",
  "description": "Generate a contributing.md, issue_template.md, or pull_request_template.md file for a project. Can be generated from the command line when Generate is installed globally, or as a plugin inside another generator.",
  "version": "1.0.0",
  "homepage": "https://github.com/generate/generate-contributing",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Jon Schlinkert (http://twitter.com/jonschlinkert)",
    "pointnet (https://github.com/pointnet)"
  ],
  "repository": "generate/generate-contributing",
  "bugs": {
    "url": "https://github.com/generate/generate-contributing/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js",
    "templates"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "generate-defaults": "^0.6.6",
    "is-valid-app": "^0.3.0"
  },
  "devDependencies": {
    "delete": "^1.1.0",
    "generate": "^0.14.0",
    "global-modules": "^1.0.0",
    "gulp-format-md": "^1.0.0",
    "mocha": "^3.5.3",
    "verb-repo-data": "^0.1.15"
  },
  "keywords": [
    "app",
    "boilerplate",
    "build",
    "cli",
    "cli-app",
    "command-line",
    "contributing",
    "create",
    "dev",
    "development",
    "framework",
    "front",
    "frontend",
    "generate",
    "generate-generator",
    "generate-plugin",
    "generategenerator",
    "generateplugin",
    "generator",
    "init",
    "initialize",
    "new",
    "plugin",
    "project",
    "projects",
    "scaffold",
    "scaffolder",
    "scaffolding",
    "template",
    "templates",
    "tool",
    "web",
    "webapp",
    "yeoman",
    "yo"
  ],
  "lintDeps": {
    "ignore": [
      "scaffolds",
      "templates"
    ],
    "dependencies": {
      "files": [
        "generator.js",
        "index.js"
      ]
    }
  },
  "verb": {
    "toc": {
      "method": "preWrite"
    },
    "layout": "generator",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "generate-eslint",
        "generate-license",
        "generate-project",
        "generate-travis"
      ]
    },
    "reflinks": [
      "assemble",
      "base",
      "gulp"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
