{
  "name": "npm-paths",
  "description": "Returns an array of unique \"npm\" directories based on the user's platform and environment.",
  "version": "0.1.4",
  "homepage": "https://github.com/jonschlinkert/npm-paths",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Brian Woodward (https://twitter.com/doowb)",
    "Jon Schlinkert (http://twitter.com/jonschlinkert)"
  ],
  "repository": "jonschlinkert/npm-paths",
  "bugs": {
    "url": "https://github.com/jonschlinkert/npm-paths/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "global-modules": "^1.0.0",
    "is-windows": "^1.0.1"
  },
  "devDependencies": {
    "gulp-format-md": "^1.0.0",
    "helper-example": "^0.1.0",
    "mocha": "^3.5.3"
  },
  "keywords": [
    "directory",
    "dirname",
    "global",
    "module",
    "npm",
    "package",
    "path",
    "paths",
    "prefix",
    "resolve"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "contains-path",
        "global-modules",
        "global-paths",
        "global-prefix",
        "look-up",
        "resolve-up"
      ]
    },
    "reflinks": [
      "global-paths",
      "verb"
    ],
    "lint": {
      "reflinks": true
    },
    "helpers": [
      "helper-example"
    ]
  }
}
