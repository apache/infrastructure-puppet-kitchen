    "pkg-store": "^0.2.2"
  },
  "keywords": [
    "api",
    "app",
    "application",
    "base",
    "baseplugin",
    "building-blocks",
    "create",
    "framework",
    "npm",
    "plugin",
    "plugins",
    "prompt",
    "tool",
    "toolkit",
    "tools"
