{
  "name": "resolve-up",
  "description": "Resolve paths to globally installed npm modules using glob patterns. Useful for getting paths to yeoman or Generate generators, etc.",
  "version": "1.0.0",
  "homepage": "https://github.com/jonschlinkert/resolve-up",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/resolve-up",
  "bugs": {
    "url": "https://github.com/jonschlinkert/resolve-up/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "array-unique": "^0.3.2",
    "extend-shallow": "^2.0.1",
    "global-modules": "^0.2.3",
    "is-valid-glob": "^0.3.0",
    "matched": "^0.4.4",
    "npm-paths": "^0.1.3"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.12",
    "mocha": "^3.4.2"
  },
  "keywords": [
    "filepath",
    "find",
    "findup",
    "get",
    "glob",
    "global",
    "install",
    "installed",
    "list",
    "match",
    "module",
    "npm",
    "package",
    "path",
    "paths",
    "resolve",
    "search",
    "up"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "highlight": "global-paths",
      "list": [
        "global-modules",
        "global-paths",
        "global-prefix",
        "findup-sync"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
