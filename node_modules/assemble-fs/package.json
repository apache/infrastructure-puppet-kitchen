    "vinyl-fs": "^2.4.4"
  },
  "devDependencies": {
    "buffer-equal": "^1.0.0",
    "default-resolution": "^2.0.0",
    "delete": "^1.0.1",
    "expect": "^1.20.2",
    "from2": "^2.3.0",
    "graceful-fs": "^4.1.11",
    "gulp-format-md": "^0.1.12",
    "mocha": "^3.4.1",
    "readable-stream": "^2.2.9",
    "rimraf": "^2.6.1",
    "should": "^11.2.1",
    "sinon": "^2.2.0",
    "templates": "^1.2.8",
    "vinyl": "^2.0.2"
  },
  "keywords": [
    "assemble",
    "assembleplugin",
