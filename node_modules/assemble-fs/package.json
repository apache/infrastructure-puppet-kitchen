    "front",
    "frontend",
    "fs",
    "plugin",
    "project",
    "projects",
    "scaffold",
    "scaffolder",
    "scaffolding",
    "system",
    "template",
    "templates",
    "vinyl",
    "webapp",
    "yeoman",
    "yo"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "assemble",
        "assemble-loader",
        "assemble-render-file",
        "assemble-streams",
        "generate",
        "verb"
